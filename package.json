{
	"name": "vanishtodo",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"preview": "bun run build && vite preview",
		"deploy": "bun run build && wrangler deploy",
		"check": "biome check --write ./src ./worker",
		"cf-typegen": "wrangler types",
		"migrate": "wrangler d1 migrations apply vanishtodo-db",
		"initdb": "wrangler d1 execute vanishtodo-db --local --file=./migrations/0001_init-schema.sql"
	},
	"dependencies": {
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.1",
		"@mui/icons-material": "^7.3.6",
		"@mui/material": "^7.3.6",
		"@mui/x-date-pickers": "^8.23.0",
		"dayjs": "^1.11.19",
		"drizzle-orm": "^0.45.0",
		"hono": "^4.10.7",
		"react": "^19.2.1",
		"react-dom": "^19.2.1",
		"react-router": "^7.10.1",
		"resend": "^6.6.0",
		"uuid": "^13.0.0",
		"valibot": "^1.2.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.8",
		"@cloudflare/vite-plugin": "^1.17.0",
		"@types/node": "^24.10.1",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"@types/uuid": "^11.0.0",
		"@vitejs/plugin-react": "^5.1.1",
		"drizzle-kit": "^0.31.8",
		"typescript": "^5.9.3",
		"vite": "^7.2.7",
		"wrangler": "^4.56.0"
	}
}
